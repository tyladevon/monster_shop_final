<h1 align = "center">Address</h1>
<center>
  <%= form_for [@user, @address] do |f| %>
    <%= f.label :nickname %>
    <%= f.text_field :nickname %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zip %>
    <%= f.text_field :zip %>

    <%= f.submit 'Submit' %>
  <% end %>
</center>
